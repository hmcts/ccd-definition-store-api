{
  "_guid_": "S-109.4",
  "_extends_": "Import_Base_data",
  "title": "Must successfully import a definition file where CaseAccessGroup fields",

  "specs": [
    "an active profile in CCD",
    "contains a valid jurisdiction id",
    "contains a valid caseType id",
    "contains id of a case type with CaseAccessGroup fields with caseAccessGroupType and caseAccessGroupId",
    "contains all details of the case type requested",
    "contains CaseAccessGroup column",
    "has the 200 OK code",
    "contains case type details"
  ],

  "request": {
    "body": {
      "arrayInMap": [
        {
          "key": "file",
          "localFilePath": "build/tmp/definition_files_copy/BEFTA_MASTER.xlsx"
        }
      ]
    }
  },

  "expectedResponse": {
    "_extends_": "Common_201_Response",
    "headers": {
      "Content-Encoding": "[[ANYTHING_PRESENT]]",
      "Content-Type": "application/json",
      "Content-Length": "[[ANYTHING_PRESENT]]"
    },
    "body": {
      "__plainTextValue__" : "Case Definition data successfully imported"
    }
  }
}
