              "displayOrder": 2,
              "roles": [
                {
                  "caseTypeId": "FT_MasterCaseType",
                  "organisationalRoleName": "Role1",
                  "groupRoleName": null,
                  "caseGroupIdTemplate": null,
                  "groupAccessEnabled": null
                }
